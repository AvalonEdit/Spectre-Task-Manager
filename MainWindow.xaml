                <StackPanel>
                    <Label x:Name="cpuLabel" Content="%" Foreground="White" VerticalAlignment="Top"/>
                    <Label x:Name="memoryLabel" Content="MB" Foreground="White" VerticalAlignment="Top"/>
                </StackPanel>
            </Grid>
        </Grid>
    </Grid>
</Window>
